{
  "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true,
      "@edgeros/jsre/jsre": true
  },
  "extends": ["standard"],
  "parserOptions": {
      "ecmaVersion": 12
  },
  "rules": {
    "no-undef":0,
    "no-new":0,
    "no-unused-vars":0
  },
  "root": true,
  "plugins": [
      "@edgeros/jsre"
  ]
}